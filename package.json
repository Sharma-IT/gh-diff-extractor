{
  "name": "gh-diff-extractor",
  "version": "1.0.0",
  "description": "A CLI tool to extract git diffs from GitHub pull request files pages, supporting private repositories",
  "main": "dist/index.js",
  "bin": {
    "gh-diff-extractor": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tsc --noEmit",
    "start": "node dist/cli.js",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "test:cli": "./test-cli.sh",
    "prepare": "npm run build"
  },
  "keywords": [
    "github",
    "git",
    "diff",
    "pull-request",
    "cli",
    "tool"
  ],
  "author": "Shubham Sharma <shubhamsharma.emails@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "axios": "^1.10.0",
    "chalk": "^5.4.1",
    "commander": "^14.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.15",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
